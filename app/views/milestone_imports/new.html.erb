<h1>New milestone_import</h1>

<% form_for(@milestone_import) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :node_id %><br />
    <%= f.text_field :node_id %>
  </p>
  <p>
    <%= f.label :milestone_id %><br />
    <%= f.text_field :milestone_id %>
  </p>
  <p>
    <%= f.label :pfa %><br />
    <%= f.text_area :pfa %>
  </p>
  <p>
    <%= f.label :filename %><br />
    <%= f.text_area :filename %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', milestone_imports_path %>